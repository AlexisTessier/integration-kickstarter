/*==============Font playfair===============*/

font_playfair(size, transform = false, weight = 400, style = normal)
	font_base 'Playfair Display' size transform
	font-weight weight
	font-style style
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;

font_playfairBlack(size, transform = false)
	font_playfair(size, transform, 900)

font_playfairBlackItalic(size, transform = false)
	font_playfair(size, transform, 900, italic)

font_playfairBold(size, transform = false)
	font_playfair(size, transform, 700)

font_playfairBoldItalic(size, transform = false)
	font_playfair(size, transform, 700, italic)

font_playfairItalic(size, transform = false)
	font_playfair(size, transform, 400, italic)
	
font_playfairRegular(size, transform = false)
	font_playfair(size, transform)

